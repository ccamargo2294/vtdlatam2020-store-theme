{
  "store.custom#lives": {
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.phone"
    ]
  },
  "responsive-layout.desktop": {
    "children": ["livestreaming-wrapper"]
  },
  "responsive-layout.phone": {
    "children": ["livestreaming-wrapper#phone"]
  },
  "livestreaming-wrapper#phone": {
    "children":[ "flex-layout.row#livestreaming-phone"]
  },
  "flex-layout.row#livestreaming-phone": {
    "children": [
      "flex-layout.col#products-phone",
      "flex-layout.col#video",
      "flex-layout.col#chat"
    ],  
    "props": {
      "blockClass": "container-live",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.col#products-phone": {
    "children": [
      "disclosure-layout#simple"
    ],
    "props": {
      "blockClass": "product-list"
    }
  },
  "disclosure-layout#simple": {
    "children": ["disclosure-trigger#simple", "disclosure-content#simple"]
  },
  "disclosure-trigger#simple": {
    "children": ["rich-text#products"]
  },
  "rich-text#products": {
    "props": {
      "text": "Products"
    }
  },
  "disclosure-content#simple": {
    "children": ["list-context.livestreaming-products#demo1"]
  },
  "livestreaming-wrapper": {
    "children":[ "flex-layout.row#livestreaming"]
  },
  "rich-text": {
    "props": {
      "text": "VTEX Store Framework",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-1"
    }
  },
  "flex-layout.row#livestreaming": {
    "children": [
      "flex-layout.col#products",
      "flex-layout.col#video",
      "flex-layout.col#chat"
    ],  
    "props": {
      "blockClass": "container-live",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.row#shelf": {
    "children": ["list-context.livestreaming-products#demo1"]
  },
  "list-context.livestreaming-products#demo1": {
    "blocks": ["product-summary.shelf#livestreaming"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "blockClass": "product-list"
    }
  },
  "slider-layout#demo-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },
  "flex-layout.col#products": {
    "children": [
      "list-context.livestreaming-products#demo1"
    ],
    "props": {
      "blockClass": "product-list"
    }
  },
  "flex-layout.col#video": {
    "children": [
      "livestreaming-component",
      "flex-layout.row#likes"
    ],
    "props": {
      "blockClass": "livestreaming"
    }
  },
  "flex-layout.row#likes": {
    "children": [
      "like-btn",
      "likes-counter"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "counter"
    }
  },
  "flex-layout.col#chat": {
    "children": [
      "chat"
    ],
    "props": {
      "blockClass": "chat"
    }
  }
}
